<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_must_be_greater_than_Start_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK( Start_Date__c )),NOT(ISBLANK( End_Date__c )), End_Date__c &lt; Start_Date__c)</errorConditionFormula>
    <errorMessage>The end date cannot be before the start date.</errorMessage>
</ValidationRule>
