<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BenefitType_is_Required_if_Other_choosed</fullName>
    <active>true</active>
    <errorConditionFormula>IF(ISPICKVAL(Benefit_1__c, &quot;Other&quot;) &amp;&amp;  ISBLANK(Other_Benefit_Type1__c) ,true,false)</errorConditionFormula>
